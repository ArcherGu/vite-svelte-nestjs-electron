{
  "name": "vite-svelte-nestjs-electron",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@10.19.0",
  "description": "Vite + Svelte + Electron with Doubleshot, so fast! âš¡",
  "main": "dist/main/index.cjs",
  "scripts": {
    "dev": "rimraf dist && vite",
    "debug": "rimraf dist && vite -- --dsb-debug",
    "build": "rimraf dist && vite build",
    "preview": "vite preview",
    "typecheck": "svelte-check --tsconfig ./tsconfig.render.json",
    "typecheck:main": "tsc -p ./tsconfig.main.json --noEmit",
    "typecheck:preload": "tsc -p ./tsconfig.preload.json --noEmit",
    "check": "npm run typecheck && npm run typecheck:main && npm run typecheck:preload",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@doubleshot/nest-electron": "^0.2.6",
    "@nestjs/common": "^11.0.9",
    "@nestjs/core": "^11.0.9",
    "@nestjs/microservices": "^11.0.9",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@lightwing/eslint-config": "^1.0.104",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@tsconfig/svelte": "^5.0.4",
    "electron": "^39.0.0",
    "electron-builder": "^26.0.0",
    "eslint": "^9.20.1",
    "eslint-plugin-svelte": "^3.0.0",
    "lint-staged": "^16.0.0",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "svelte": "^5.20.0",
    "svelte-check": "^4.1.4",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "vite": "^7.0.0",
    "vite-plugin-doubleshot": "^0.0.19"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@nestjs/core",
      "electron"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md,json,yml}": [
      "eslint --fix"
    ]
  }
}
