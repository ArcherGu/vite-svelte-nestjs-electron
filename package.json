{
  "name": "vite-svelte-nestjs-electron",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@10.30.3",
  "description": "Vite + Svelte + Electron with Doubleshot, so fast! âš¡",
  "main": "dist/main/index.cjs",
  "scripts": {
    "dev": "rimraf dist && vite",
    "debug": "rimraf dist && vite -- --dsb-debug",
    "build": "rimraf dist && vite build",
    "preview": "vite preview",
    "typecheck": "svelte-check --tsconfig ./tsconfig.render.json",
    "typecheck:main": "tsc -p ./tsconfig.main.json --noEmit",
    "typecheck:preload": "tsc -p ./tsconfig.preload.json --noEmit",
    "check": "npm run typecheck && npm run typecheck:main && npm run typecheck:preload",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postinstall": "npx simple-git-hooks && electron-builder install-app-deps"
  },
  "dependencies": {
    "@doubleshot/nest-electron": "^0.2.11",
    "@nestjs/common": "^11.1.14",
    "@nestjs/core": "^11.1.14",
    "@nestjs/microservices": "^11.1.14",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@lightwing/eslint-config": "^1.0.147",
    "@sveltejs/vite-plugin-svelte": "^7.0.0",
    "@tsconfig/svelte": "^5.0.8",
    "electron": "^40.6.1",
    "electron-builder": "^26.8.1",
    "eslint": "^10.0.2",
    "eslint-plugin-svelte": "^3.15.0",
    "lint-staged": "^16.2.7",
    "rimraf": "^6.1.3",
    "simple-git-hooks": "^2.13.1",
    "svelte": "^5.53.6",
    "svelte-check": "^4.4.4",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-doubleshot": "^0.0.20"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@nestjs/core",
      "electron",
      "electron-winstaller",
      "esbuild"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md,json,yml}": [
      "eslint --fix"
    ]
  }
}
