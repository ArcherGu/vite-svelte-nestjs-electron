{
  "name": "vite-svelte-nestjs-electron",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@9.7.1",
  "description": "Vite + Svelte + Electron with Doubleshot, so fast! âš¡",
  "main": "dist/main/index.cjs",
  "scripts": {
    "dev": "rimraf dist && vite",
    "debug": "rimraf dist && vite -- --dsb-debug",
    "build": "rimraf dist && vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@doubleshot/nest-electron": "^0.2.5",
    "@nestjs/common": "^10.3.8",
    "@nestjs/core": "^10.3.8",
    "@nestjs/microservices": "^10.3.8",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@lightwing/eslint-config": "^1.0.52",
    "@sveltejs/vite-plugin-svelte": "^3.1.0",
    "@tsconfig/svelte": "^5.0.4",
    "electron": "^32.0.0",
    "electron-builder": "^24.13.3",
    "eslint": "^9.2.0",
    "eslint-plugin-svelte": "^2.39.0",
    "lint-staged": "^15.2.2",
    "rimraf": "^6.0.0",
    "simple-git-hooks": "^2.11.1",
    "svelte": "^4.2.17",
    "svelte-check": "^3.7.1",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-doubleshot": "^0.0.16"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md,json,yml}": [
      "eslint --fix"
    ]
  }
}
